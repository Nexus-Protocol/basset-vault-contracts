{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GovernanceMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "update_config"
      ],
      "properties": {
        "update_config": {
          "type": "object",
          "properties": {
            "governance_contract_addr": {
              "type": [
                "string",
                "null"
              ]
            },
            "psi_token_contract_addr": {
              "type": [
                "string",
                "null"
              ]
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "update_rewards_distribution"
      ],
      "properties": {
        "update_rewards_distribution": {
          "type": "object",
          "required": [
            "distribution"
          ],
          "properties": {
            "distribution": {
              "type": "array",
              "items": {
                "type": "array",
                "items": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "integer",
                    "format": "uint64",
                    "minimum": 0.0
                  }
                ],
                "maxItems": 2,
                "minItems": 2
              }
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
